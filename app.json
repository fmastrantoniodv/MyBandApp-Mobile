{
  "expo": {
    "name": "My band app",
    "slug": "my-band-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "mybandapp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.frankiemybandapp.mybandapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "ENDPOINT_BACKEND": "https://mybandapp-backend.onrender.com",
      "ENV_CODE": "DEV",
      "LOCAL_ENDPOINT_BACKEND": "http://10.0.2.2:3001",
      "TIMEOUT_SERVICES": 15000,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d2ccb803-0e91-4884-9481-218a4379438d"
      }
    }
  }
}
